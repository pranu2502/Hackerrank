#include<stdio.h>

void divisiblesumpairs(array[],n)
{
    for(i = 0;i<n;i++)
    {
        for(j = i+1;j<n;j++)
        {
            if((array[i]+array[j])%k == 0)
            {
                count++;
            }
        }
    }
    printf("%d",count);
}

int main()
{
    int n,k,i,j,count = 0;
    int array[100];
    scanf("%d %d",&n,&k);
    for(i = 0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    divisiblesumpairs(array,n);
    return 0;
}
