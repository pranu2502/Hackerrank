#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    scanf("%d",&n);
    while(n > 0)
    {
        int x,y,z;
        scanf("%d %d %d",&x,&y,&z);
        int dist_a = abs(z - x);
        int dist_b = abs(y - z);
        if(dist_a < dist_b)
        {
            printf("Cat A");
        }
        else if(dist_a > dist_b)
        {
            printf("Cat B");
        }
        else
        {
            printf("Mouse C");
        }
        printf("\n");
        n--;
    }
    return 0;
}
